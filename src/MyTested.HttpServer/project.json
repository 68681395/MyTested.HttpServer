{
  "version": "1.2.3",
  "title": "MyTested.HttpServer",
  "copyright": "2015-2016 Ivaylo Kenov",
  "description": "MyTested.HttpServer is a unit testing library providing easy fluent interface to test remote HTTP servers. See the project repository and documentation for more information and available features.",
  "authors": [ "Ivaylo Kenov" ],

  "packOptions": {
    "summary": "Fluent testing framework for remote HTTP servers.",
    "tags": [ "fluent", "testing", "framework", "unit tests", "test", "http", "server", "remote" ],
    "iconUrl": "https://raw.githubusercontent.com/ivaylokenov/MyTested.HttpServer/master/documentation/nuget-logo.png",
    "projectUrl": "https://github.com/ivaylokenov/MyTested.HttpServer",
    "licenseUrl": "https://github.com/ivaylokenov/MyTested.HttpServer/blob/master/LICENSE"
  },

  "buildOptions": {
    "warningsAsErrors": true
  },

  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Net.Http": "*"
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}